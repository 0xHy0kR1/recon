    <script>
        // CSRF protection code
        // Based on https://docs.djangoproject.com/en/dev/ref/contrib/csrf/#ajax
        function getCookie(name) {
         var cookieValue = null;
         if (document.cookie && document.cookie != '') {
           var cookies = document.cookie.split(';');
           for(var i = 0; i < cookies.length; i++) {
             var cookie = cookies[i].trim();
             // Does this cookie string begin with the name we want?
             if(cookie.substring(0, name.length + 1) == (name + '=')) {
         cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
         break;
             }
           }
         }
         return cookieValue;
        }

        var csrftoken = getCookie('csrftoken');

        $(window).on('load', function() {
            window.ajaxLogger = new AJAXLogger("/jsv/ajax/log/metric/", csrftoken, window.django_jsvlogger_logWithBeacon);
            window.ajaxLogger.bindEvents();
            window.rcv = new RCV({
                csrftoken: csrftoken,
                ref_urlpath: '/',
                ref_uid: '1i1v8b8eo0000ej9',
                urls: {
                    jsv: "/jsv/ajax/verify/jsv/",
                    jsvimage: "/jsv/ajax/image/",
                    interaction: "/jsv/ajax/verify/interaction/",
                    time_on_page: "/jsv/ajax/time_on_page/",
                    performance_timing: "/jsv/ajax/performance_timing/",
                    ctk: "/jsv/ajax/verify/ctk/",
                    ctkimage: "https://www.indeed.com/hire/cookie"
                },
                status: {
                    jsv: false,
                    interaction: false,
                    ctk: false
                },
                performance_timing: {
                    enabled: false,
                    fields: [
                        "redirectStart","redirectEnd","fetchStart","domainLookupStart","domainLookupEnd","connectStart","connectEnd","secureConnectionStart","requestStart","responseStart","responseEnd","unloadEventStart","unloadEventEnd","domInteractive","domContentLoadedEventStart","domContentLoadedEventEnd","domComplete","loadEventStart","loadEventEnd","firstPaint","firstContentfulPaint",
                    ]
                },
                useBeacon: window.django_jsvlogger_logWithBeacon
            });
            window.rcv.run();
        });
